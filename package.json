    "jquery": "^3.3.1",
    "json-server": "^0.16.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "vue": "^2.6.6",
    "vue-axios": "^3.2.0",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^3.0.2",
    "vuejs-jwt": "^1.1.0",
    "zoomus-jssdk": "^1.3.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^4.5.9",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
